<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/m0705_r001"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back05"
    android:padding="@dimen/p10"
    android:clipChildren="false"
    tools:context=".M0705">

    <TextView
        android:id="@+id/m0705_t001"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="10dp"
        android:background="@color/Yellow"
        android:gravity="center"
        android:text="@string/m0705_t001"
        android:textColor="@color/Maroon"
        android:textSize="@dimen/fontsize" />

    <TextView
        android:id="@+id/m0705_t002"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0705_t001"
        android:layout_alignParentStart="true"
        android:layout_marginStart="@dimen/p10"
        android:layout_marginTop="@dimen/p10"
        android:background="@color/Aqua"
        android:text="@string/m0705_t002"
        android:textSize="@dimen/fontsize" />

    <TextView
        android:id="@+id/m0705_t003"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/m0705_t002"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="@dimen/p10"
        android:background="@color/Aqua"
        android:text="@string/m0705_t003"
        android:textSize="@dimen/fontsize" />

    <ImageButton
        android:id="@+id/m0705_b001"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0705_t003"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="@dimen/Margins"
        android:layout_marginEnd="@dimen/Margins"
        android:background="@drawable/circle_shape"
        android:src="@drawable/scissors"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/m0705_b002"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0705_b001"
        android:layout_alignEnd="@+id/m0705_b001"
        android:layout_marginTop="@dimen/Margins"
        android:background="#00000000"
        android:src="@drawable/stone"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/m0705_b003"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0705_b002"
        android:layout_alignEnd="@+id/m0705_b002"
        android:layout_marginTop="@dimen/Margins"
        android:background="#00000000"
        android:src="@drawable/net"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageSwitcher
        android:id="@+id/m0705_c001"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/m0705_t002"
        android:layout_alignTop="@+id/m0705_b002"
        android:layout_marginStart="@dimen/p10"
        android:gravity="center"
        android:text="@string/m0705_c001"
        android:textSize="@dimen/fontsize"

        app:srcCompat="@drawable/scissors"
        />

    <TextView
        android:id="@+id/m0705_s001"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0705_b003"
        android:layout_alignParentStart="true"
        android:layout_marginStart="@dimen/p10"
        android:layout_marginTop="@dimen/p10"
        android:layout_marginEnd="@dimen/p10"
        android:layout_marginBottom="@dimen/p10"
        android:background="@color/Silver"
        android:text="@string/m0705_s001"
        android:textColor="@color/Red"
        android:textSize="@dimen/fontsize"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/m0705_f000"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0705_s001"
        android:layout_alignParentStart="true"
        android:layout_marginStart="@dimen/Margins"
        android:layout_marginTop="@dimen/Margins"
        android:layout_marginEnd="@dimen/Margins"
        android:layout_marginBottom="@dimen/Margins"
        android:background="@color/Silver"
        android:text="@string/m0705_f000"
        android:textSize="@dimen/fontsize" />

    <TextView
        android:id="@+id/myname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="@dimen/p20"
        android:layout_marginBottom="@dimen/p20"
        android:text="@string/myname"
        android:textColor="@color/White"
        android:textSize="@dimen/fontsize" />

</RelativeLayout>